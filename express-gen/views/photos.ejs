<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta http-equiv="X-UA-Compatible" content="ie=edge">
  <link rel='stylesheet' href='/stylesheets/photos.css' />
  <title>Photos List</title>
</head>
<body>
  <h1><%= title %></h1>
  <p>Express stock photo application</p>
  <div class="photos">
    <% photosData.forEach(function(photo){ %>
      <div class="photo">
        <h3><%= photo %></h3>
        <img src='<%= `/images/${photo}` %>' />
      </div>
    <% }) %>
  </div>
</body>
</html>